	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Michal Lulco",
			"email": "michal.lulco@gmail.com"
		}
	],
	"require": {
		"php": ">= 5.6.0"
	},
	"require-dev": {
